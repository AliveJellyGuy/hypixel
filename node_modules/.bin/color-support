#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules/color-support/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/node_modules:/home/floriane/.local/share/mcpelauncher/games/node_modules:/home/floriane/.local/share/mcpelauncher/node_modules:/home/floriane/.local/share/node_modules:/home/floriane/.local/node_modules:/home/floriane/node_modules:/home/node_modules:/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules/color-support/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/node_modules:/home/floriane/.local/share/mcpelauncher/games/node_modules:/home/floriane/.local/share/mcpelauncher/node_modules:/home/floriane/.local/share/node_modules:/home/floriane/.local/node_modules:/home/floriane/node_modules:/home/node_modules:/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../color-support/bin.js" "$@"
else
  exec node  "$basedir/../color-support/bin.js" "$@"
fi
