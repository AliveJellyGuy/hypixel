#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules/.pnpm/which@1.3.1/node_modules/which/bin/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules/.pnpm/which@1.3.1/node_modules/which/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules/.pnpm/which@1.3.1/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules/.pnpm/which@1.3.1/node_modules/which/bin/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules/.pnpm/which@1.3.1/node_modules/which/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules/.pnpm/which@1.3.1/node_modules:/home/floriane/.local/share/mcpelauncher/games/com.mojang/development_behavior_packs/hypixel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/which" "$@"
else
  exec node  "$basedir/../../bin/which" "$@"
fi
